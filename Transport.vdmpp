class Transport

types
	public TypeOfVehicle = <Motorcycle> | <Ambulance> | <EmergencyCar> | <Heli>;

instance variables
	public type: TypeOfVehicle;
	public velocity: real;
	
operations
    
    public Transport: TypeOfVehicle  ==> Transport
    Transport(t) == 
    (
        type := t;
                
        return self;
    )
    post type = t;

end Transport